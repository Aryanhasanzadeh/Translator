{
    "name": "aryanhasanzadeh/translator",
    "description": "This Package Help to Manage Translator Server and Translate data for Laravel Projects",
    "keywords": ["google", "translate", "translator", "php","laravel","translate Manager","api","laravel","laravel-package"],
    "homepage": "https://github.com/Aryanhasanzadeh/Translator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aryan Hasanzadeh",
            "email": "aryanhasanzadeh75@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "stichoza/google-translate-php": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aryanhasanzadeh\\Translator\\": "src/",
            "Aryanhasanzadeh\\Translator\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aryanhasanzadeh\\Translator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aryanhasanzadeh\\Translator\\Providers\\TranslatorServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/Aryanhasanzadeh/Translator/issues",
        "wiki": "https://github.com/Aryanhasanzadeh/Translator/wiki",
        "source": "https://github.com/Aryanhasanzadeh/Translator"
    }
}
